val pp_set_ellipsis_text : formatter -> string -> unit


val set_ellipsis_text : string -> unit[@@ocaml.doc
                                        " Set the text of the ellipsis printed when too many pretty-printing boxes\n  are open (a single dot, [.], by default).\n"]


